            "Override": {
                "System": "Verbose",
                "Microsoft": "Verbose",
                "Vulkan.DebugMessenger": "Verbose",
                "Vulkan.DebugMessenger.General": "Verbose",
                "Aliquip.MemoryFactory": "Verbose",
